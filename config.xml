        <variable name="FABRIC_API_KEY" value="dae4b00e0e8a1fdd59520735a3842fe2841a0596" />
        <variable name="FABRIC_API_SECRET" value="19e6b239a7478a278bdbccc7989ac1a2d25a1852c15fa17bd423c7ce02019fef" />
